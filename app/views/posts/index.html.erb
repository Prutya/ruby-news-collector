<h1>Posts#index</h1>
<p>Find me in app/views/posts/index.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>News Id</th>
      <th>Author</th>
      <th>Title</th>
      <th>Url</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.id %></td>
        <td><%= post.website_id %></td>
        <td><%= post.author %></td>
        <td><%= post.title %></td>
        <td><%= post.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @posts %>
